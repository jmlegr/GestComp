/* Default style for text in accordion panes. */
.cssDefault {
  font-size : 10pt;
  font-family : arial;
}


/* Style for text in please wait dialog. */
.cssPleaseWait {
  font-size : 12pt;
  font-weight : bold;
  font-family : arial;
}


/* Style for text in about dialog. */
.cssAbout {
  font-size : 11pt;
  font-weight : bold;
  font-family : arial;
}


/* Style for source of dialog animations. */
.cssSource {
  position : absolute;
  left: 1px;
  top : 1px;
  width : 1px;
  height : 1px;
}


/* Style for table showing item details. */
.cssDetailsTable {
  border : 2px solid #99bbe8;
  padding : 2px;
}


/* Style for field label in table showing item details. */
.cssDetailsLabel {
  background-color : #c7dffc;
  font-size : 10pt;
  font-family : arial;
}


/* Style for field data in table showing item details. */
.cssDetailsData {
  background-color : #f4f4f4;
  font-weight : bold;  
  font-size : 12pt;
  font-family : arial;
}
.cssDetailsDataMini {
  background-color : #f4f4f4;
  font-size : 11pt;
  font-family : arial;
}



/* Style for dataview body (icon text mainly). */
#listingCard .x-panel-body {
  background : white;
  font-size : 8pt;
  font-family : arial;
}



/* Style for dataview thumbnails. */
#listingCard .thumb-wrap {
  float : left;
  margin : 4px;
  margin-right : 0;
  padding : 5px;
}


/* Style for dataview item text. */
#listingCard .thumb-wrap span {
  display : block;
  overflow : hidden;
  text-align : center;
}


/* Style for when an icon in dataview is hovered over. */
#listingCard .x-view-over {
  border :1px solid #dddddd;
  background : #efefef;
  padding: 4px;
}


/* Style for when an icon in dataview is selected. */
#listingCard .x-view-selected {
  background : #eff5fb;
  border : 1px solid #99bbe8;
  padding : 4px;
}


/* Fix for ComboBox arrow moving error in v3.0. */
.x-form-field-wrap {
  position : static;
}
.x-form-field-wrap .x-form-trigger {
  position : static;
  top : auto;
  vertical-align : middle;
}
.x-form-field-wrap .x-form-twin-triggers .x-form-trigger {
  position : static;
  top : auto;
  vertical-align : middle;
}

.gridBodyNotifyOver {
border-color: #00cc33 !important;
}
.gridRowInsertBottomLine {
border-bottom:1px dashed #00cc33;
}
.gridRowInsertTopLine {
border-top:1px dashed #00cc33;
}
